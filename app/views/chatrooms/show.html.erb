<p>
  <strong>Name:</strong>
  <%= @chatroom.name %>
</p>

<div data-behavior="messages">
  <% 1.upto(5) do %>
    <div> <strong><%= current_user.username %> </strong> Message!</div>
    
  <% end %>
</div>

<%= form_for [@chatroom, Message.new] do |f| %>
  <%= f.text_area :body, rows: 2, class: 'form-control', autofocus: true %>
<% end %>